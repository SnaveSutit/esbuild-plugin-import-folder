{
	"typeAcquisition": {
		"enable": true
	},
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "node",
		"strict": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	}
}
